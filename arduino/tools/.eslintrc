{
  "parser": "babel-eslint",

  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },

  "extends": [
    "airbnb-base"
  ],

  "rules": {
    "semi": "error",
    "no-use-before-define": "off",
    "no-console":"off"
  },

  "env":{
    "node": true,
    "browser": true
  }
}